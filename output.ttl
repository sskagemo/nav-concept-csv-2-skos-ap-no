            dct:source [ rdfs:label "Oversatt fra New Interoperability Framework for European Public Services, s. 28. European Commission, EC. (2017). Hentet 21. juli 2017 fra https://ec.europa.eu/isa2/sites/isa/files/eif_brochure_final.pdf"@nb ] ;
            dct:type "Faglig definisjon"@nb ;
