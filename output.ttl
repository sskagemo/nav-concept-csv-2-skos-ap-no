    dct:publisher <https://data.brreg.no/enhetsregisteret/api/enheter/889640782> ;
    skos:example "- Bruker sender inn et elektronisk spørsmål til NAV, og får en automatisk bekreftelse før NAV senere besvarer henvendelsen i samme kanal."@nb ;
    skosxl:prefLabel [ a skosxl:Label ;
            skosxl:literalForm "Bekreftelse"@nb ] .

